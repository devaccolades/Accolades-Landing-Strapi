{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "Blog"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "coverImage": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images"
      ]
    },
    "innerImage": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images"
      ]
    },
    "time": {
      "type": "date"
    },
    "readTime": {
      "type": "integer"
    },
    "blog_category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::blog-category.blog-category",
      "inversedBy": "blogs"
    },
    "category": {
      "type": "string"
    }
  }
}
